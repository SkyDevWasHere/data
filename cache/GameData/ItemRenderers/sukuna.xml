				<Frame>2,2</Frame>
				<Frame>3,2</Frame>
				<Frame>4,2</Frame>
				<Frame>5,2</Frame>
			</SpriteAnimation>
			<SpriteAnimation name="ge_id_punch" isLoop="true" sprite="md_sprite" animTime="400" playOnState="Punch.True">
				<Frame>2,4</Frame>
				<Frame>3,4</Frame>
				<Frame>4,4</Frame>
			</SpriteAnimation>
			<SpriteAnimation name="ge_id_jump_fall" playOnState="noone.Jump|noone.Fall" sprite="md_sprite" animTime="800" isLoop="true">
				<Frame>0,3</Frame>
			</SpriteAnimation>
			<SpriteAnimation name="ge_id_walk" playOnState="Motion.Move" sprite="md_sprite" animTime="800" isLoop="true">
				<Frame>0,3</Frame>
				<Frame>1,3</Frame>
				<Frame>2,3</Frame>
				<Frame>3,3</Frame>
				<Frame>4,3</Frame>
				<Frame>5,3</Frame>
				<Frame>6,3</Frame>
				<Frame>7,3</Frame>

			</SpriteAnimation>
		</Animations>
		<ParticleSystems>
