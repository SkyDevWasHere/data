			<SpriteAnimation name="ge_punch_eff" sprite="md_punch" animTime="800" isLoop="true" playOnState="Punch.True">
				<Frame>0,3</Frame>
				<Frame>1,3</Frame>
				<Frame>2,3</Frame>
				<Frame>3,3</Frame>
				<Frame>4,3</Frame>
				<Frame>5,3</Frame>
				<Frame>6,3</Frame>
				<Frame>7,3</Frame>
			</SpriteAnimation>
			<SpriteAnimation name="ge_id_before" sprite="md_sprite" animTime="1600" autoPlay="true" playOnState="jogo.Bf">
				<Frame>0,0</Frame>
				<Frame>1,0</Frame>
				<Frame>2,0</Frame>
				<Frame>3,0</Frame>
				<Frame>4,0</Frame>
				<Frame>5,0</Frame>
				<Frame>0,1</Frame>
				<Frame>1,1</Frame>
				<Frame>2,1</Frame>
				<Frame>3,1</Frame>
			</SpriteAnimation>
			<SpriteAnimation name="ge_id_after" sprite="md_sprite" animTime="800" isLoop="true" playOnState="jogo.Idle">
				<Frame>0,2</Frame>
