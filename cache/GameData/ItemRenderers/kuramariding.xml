<?xml version="1.0" encoding="utf-8"?>
<ItemRenderer item="Males-Ps|Design By Lucit">
	<Data>
		<Sprite name="foot1_Sprite" fileName="game/kuramariding.rttex" textureSize="420,360"/>
		<Sprite name="foot2_Sprite" fileName="game/kuramariding.rttex" textureSize="420,360"/>
		
		<Animations>
			<SpriteAnimation name="foot1_Idle" playOnAction="IDLE|WALK|JUMP|FALL|PUNCH|JUMPEND" sprite="foot1_Sprite" animTime="800" isLoop="true">
				<Frame>0,0</Frame>
				<Frame>2,0</Frame>
				<Frame>4,0</Frame>
				<Frame>6,0</Frame>
				<Frame>8,0</Frame>
				<Frame>10,0</Frame>
				<Frame>12,0</Frame>
				<Frame>14,0</Frame>
			</SpriteAnimation>
			<SpriteAnimation name="foot2_Idle" playOnAction="IDLE|WALK|JUMP|FALL|PUNCH|JUMPEND" sprite="foot2_Sprite" animTime="800" isLoop="true">
				<Frame>1,0</Frame>
				<Frame>3,0</Frame>
				<Frame>5,0</Frame>
				<Frame>7,0</Frame>
				<Frame>9,0</Frame>
				<Frame>11,0</Frame>
				<Frame>13,0</Frame>
				<Frame>15,0</Frame>
			</SpriteAnimation>
		</Animations>
		
		<RenderOptions>
			<Default>
				<Set name="JumpEndStateRange">100</Set>
				<Set name="ClothingOffsetFromShoes">-140</Set>
				<Set name="IsRidingBird">true</Set>
				<Set name="IsVehicle">true</Set>
				<Set name="IsPulseShoes">true</Set>
				<Set name="IsHorse">true</Set>
				<Set name="IgnorePunchTypeCheck">true</Set>
				<Set name="UserNameOffset">80</Set>
				<Set name="RenderShoesWithMermaidTail">true</Set>
			</Default>
			<Override overrideOnAction="IDLE">
				<Set name="IsPulseShoes">false</Set>
			</Override>
		</RenderOptions>
	</Data>

	<RenderRules>
		<RenderShoes>
			<SpriteRender name="foot1_Sprite" offset="0,145" alignmentLeft="DOWN_LEFT" alignmentRight="DOWN_RIGHT" drawShadow="false"/>
			<SpriteRender name="foot2_Sprite" offset="0,145" alignmentLeft="DOWN_RIGHT" alignmentRight="DOWN_LEFT" drawShadow="false"/>
		</RenderShoes>
	</RenderRules>
</ItemRenderer>